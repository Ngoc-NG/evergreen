{
  "name": "evergreen-services",
  "description": "Evergreen hosted service layer",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^9.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run jest",
    "eslint": "eslint src/. test/. acceptance/. cli-test/. cli/. --config .eslintrc.json",
    "start": "node src/",
    "jest": "jest --bail --forceExit ./test",
    "acceptance": "jest -i --bail --coverageDirectory=coverage/acceptance --forceExit ./acceptance"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.7",
    "@feathersjs/authentication-jwt": "^2.0.1",
    "@feathersjs/cli": "^3.8.0",
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.3",
    "@feathersjs/feathers": "^3.1.7",
    "@feathersjs/socketio": "^3.2.2",
    "compression": "^1.7.3",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.4",
    "ejs": "^2.6.1",
    "elliptic": "^6.4.1",
    "feathers-memory": "^2.1.3",
    "feathers-sequelize": "^3.1.2",
    "feathers-swagger": "^0.7.2",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "object-hash": "^1.3.0",
    "pg": "^7.4.3",
    "raven": "^2.6.3",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.3.2",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "compare-versions": "^3.3.0",
    "eslint": "^4.19.1",
    "hoek": "^5.0.4",
    "jest": "^22.4.4",
    "js-yaml": "^3.12.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "yargs": "^12.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/sequelize-swagger.js",
      "<rootDir>/src/sequelize.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 87,
        "branches": 63,
        "functions": 70,
        "lines": 87
      }
    }
  }
}
